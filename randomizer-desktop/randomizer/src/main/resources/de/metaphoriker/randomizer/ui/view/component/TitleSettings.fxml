<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="488.0" prefWidth="140.0" styleClass="action-settings" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.metaphoriker.randomizer.ui.view.component.TitleSettingsController">
    <children>
        <VBox fx:id="actionSettingsVBox" minHeight="488.0" minWidth="140.0" styleClass="action-settings-vbox">
            <children>
                <Label fx:id="actionInFocusLabel" styleClass="sequence-in-focus-label" text="Name Changer" />
                <Separator prefWidth="140.0" />
                <VBox prefWidth="140.0" styleClass="sequence-title-vbox">
                    <children>
                        <Label styleClass="setting-description" text="Set the title/name of the ActionSequence" wrapText="true" />
                        <TextField fx:id="textField" promptText="Set a name" styleClass="title-settings-textfield" />
                    </children>
               <padding>
                  <Insets left="8.0" right="8.0" />
               </padding>
                </VBox>
                <Separator prefWidth="140.0" />
            </children>
        </VBox>
    </children>
</VBox>
