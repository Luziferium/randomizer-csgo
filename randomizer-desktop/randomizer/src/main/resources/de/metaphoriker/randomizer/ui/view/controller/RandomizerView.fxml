<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/22"
            fx:controller="de.metaphoriker.randomizer.ui.view.controller.RandomizerViewController">
    <children>
        <VBox prefHeight="500.0"
              prefWidth="700.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
              AnchorPane.topAnchor="0.0">
            <children>
                <HBox alignment="CENTER_RIGHT" prefHeight="50.0" prefWidth="200.0">
                    <children>
                        <Button mnemonicParsing="false" text="Button"/>
                    </children>
                </HBox>
                <BorderPane prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
                    <center>
                        <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
                            <children>
                                <HBox prefHeight="25.0" prefWidth="200.0">
                                    <children>
                                        <Label text="Label"/>
                                    </children>
                                </HBox>
                                <HBox prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
                                    <children>
                                        <VBox prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS"/>
                                        <VBox prefHeight="200.0" prefWidth="50.0">
                                            <children>
                                                <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0"
                                                      VBox.vgrow="ALWAYS">
                                                    <children>
                                                        <Button mnemonicParsing="false" text="Button"/>
                                                        <Button mnemonicParsing="false" text="Button"/>
                                                    </children>
                                                </VBox>
                                            </children>
                                        </VBox>
                                    </children>
                                </HBox>
                            </children>
                        </VBox>
                    </center>
                    <top>
                        <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER"/>
                    </top>
                    <left>
                        <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER"/>
                    </left>
                    <right>
                        <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER"/>
                    </right>
                    <bottom>
                        <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER"/>
                    </bottom>
                </BorderPane>
            </children>
        </VBox>
    </children>
</AnchorPane>
